    "extra": {
        "branch-version": "dev-master",
        "patches": {
            "drupal/core": {
                "Create views plugins for date range fields": "https://www.drupal.org/files/issues/2019-04-15/2924061-14.patch",
                "Views attachments are not rendered in the specified order": "https://www.drupal.org/files/issues/2018-08-21/2943293-13.patch",
                "Broken title in modal dialog when title is a render array": "https://www.drupal.org/files/issues/2019-10-21/2663316-76.drupal.Broken-title-in-modal-dialog-when-title-is-a-render-array.patch",
                "Field tokens for 'historical data' fields (revisions) contain a hyphen, breaking twig templates and throwing an assertion error": "https://www.drupal.org/files/issues/2019-11-11/2831233-83.patch",
                "Add new permission to view user email field": "https://www.drupal.org/files/issues/2020-03-31/2989745-73.patch",
                "Views - fix Max-Date in Between operator": "https://www.drupal.org/files/issues/2019-04-02/2842409-fix-max-time-10.patch",
                "Views - fix profile relationships": "web/patches/views/2799479-65-views-rel-fix.patch",
                "Fix Form Error messages display": "https://www.drupal.org/files/issues/2019-01-21/form_system-undefined_index-3027240-2.patch",
                "Invalid argument supplied for foreach() Migration.php:634": "https://www.drupal.org/files/issues/2020-02-01/2978664-14-migration.patch",
                "viewsfieldEntityOperations::getEntityTranslation() throws exception on null": "https://www.drupal.org/files/issues/2018-09-05/2951493-15.patch",
                "Syntax error or access violation": "https://www.drupal.org/files/issues/2018-10-18/1803886-44-rollback_to_savepoint.patch",
                "Use READ COMMITTED by default for MySQL transactions": "https://www.drupal.org/files/issues/2020-05-04/1650930-default-mysql-transactions-84-D9.patch"
            },
            "drupal/chosen": {
                "Allow use of npm chosen-js library": "web/patches/chosen/0001-Add-compatibility-for-npm-chosen-js-library.patch"
            },
            "drupal/ckeditor_config": {
                "Correct handling of False value in configuration": "https://www.drupal.org/files/issues/2018-05-04/setting-configuration-value-as-false-evaluates-to-true-2969666-12601379.patch"
            },
            "drupal/responsive_menu": {
                "Displaying mobile menu fullscreen": "web/patches/responsive_menus/fullscreen.patch"
            },
            "drupal/maxlength": {
                "Removing Timeout values in main js file.": "web/patches/maxlength/removing-performance-issue.patch"
            },
            "drupal/content_lock": {
                "Fix to play nice with Save_Edit module": "web/patches/content_lock/save-edit-conflict.patch"
            },
            "drupal/profile": {
                "Add token support": "https://www.drupal.org/files/issues/add_token_support-2718597-9.patch"
            },
            "drupal/change_pwd_page": {
                "Integration with password_policy": "https://www.drupal.org/files/issues/2933747-4.patch"
            },
            "drupal/ultimate_cron": {
                "The ultimate_cron_job entity type does not exist": "https://www.drupal.org/files/issues/ultimate_cron_job-2778357-06-8.x-2.x.patch"
            },
            "drupal/entity_reference_revisions": {
                "Entity Reference Revisions - Views - fix profile relationships": "https://www.drupal.org/files/issues/2020-02-11/2799479-108-no-tests.patch"
            },
            "drupal/views_data_export": {
                "Views Data Export - fix link url with pager": "https://www.drupal.org/files/issues/views_data_export_pager-2478463-7.patch"
            },
            "drupal/video": {
                "Add source and destination module for migrate plugin": "https://www.drupal.org/files/issues/2018-07-18/2986682-2.patch"
            },
            "drupal/login_destination": {
                "Fix for current url being added to query string": "https://www.drupal.org/files/issues/2019-02-07/login_destination-only_alter_specific_link_routes-3031467-2.patch"
            },
            "drupal/views_infinite_scroll": {
                "Append new rows into HTML list (do not create new list for next page)": "https://www.drupal.org/files/issues/2019-05-10/3042195-5-append-new-rows-list.patch"
            },
            "drupal/redirect": {
                "Redirect not working for path alias":"web/patches/redirect/redirect_path_alias_not_working.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush":"https://www.drupal.org/files/issues/2018-10-14/support-for-export-2857247-24.patch"
            }

        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    },
    "require-dev": {
        "kint-php/kint": "^3.3"
    }
}
